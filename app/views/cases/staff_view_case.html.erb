    <div id="aside">
      <ul>
		<li><%= link_to "Feedback List", cus_new_case_path, :class=>"tab-active", :style=>"padding-left:50px; padding-right:60px;" %></li>
		<% if current_user.level=="Senior"%>
        <li><%= link_to "Add New Account", natas_add_new_user_path %></li>
        <% end %>
	  </ul>
    </div>
    
    <div id="content">
    	<div id="view_case">
  <%= form_for(:cases, :url=>staff_view_case_path(:type=>"filter")) do |f| %>
  <% if params[:type]=="filter"%>
    <%key=params[:nature]%>
    <h3>Category: <%= select_tag(:nature, options_for_select(CaseCategory.all.collect {|p| [ p.name.strip(), p.id ] },key)) %>
  <% else%>
    <h3>Category: <%= select_tag(:nature, options_for_select(CaseCategory.all.collect {|p| [ p.name.strip(), p.id ] })) %>
  <% end %>  
    <%= f.submit "Filter" %></h3>
<% end %><br /><br />

<% if current_user.user_group=="natas"%>
  <% if current_user.poc=="Y"%>
    <table>
    <% @cases.each do|one| %>
     <table cellspacing="10">
     <% if one.status=="0"%>     
       <tr><td><strong><%=link_to one.case_id,case_processing_path(:case_id=>one.case_id) %></strong></td>
           <td><strong><%= one.content%></strong></td></tr>    
     <% else %>
       <tr><td><%=link_to one.case_id,case_processing_path(:case_id=>one.case_id) %></td>
           <td><%= one.content%></td></tr>
     <% end %>
     </table><br />
    <% end %>    
  <% else %>  
	Responsible Cases<br />
	<table cellspacing="10">
	<% @cases.each do |one|%>
	    <tr><td><%= link_to one.content, case_processing_path(:case_id=>one.case_id) %></td><td>Send by <%= one.created_by.strip()%> on <%= one.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td></tr>
	<% end %></table><br />	
	<br />Other Cases
	<table cellspacing="10">
	<% @cases_all.each do |one|%>
	  <% if one.poc!=current_user.user_id.strip()%>
	    <tr><td><%= link_to one.content, case_processing_path(:case_id=>one.case_id) %></td><td>Send by <%= one.created_by.strip()%> on <%= one.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td></tr>
	  <% end %>
	<% end %></table><br />
  <% end %>	
<% elsif current_user.user_group != "natas" && current_user.user_group != "customer" && current_user.user_group != "admin" %>
	Responsible Cases<br />
	<table cellspacing="10">
	<% @cases.each do |one|%>
	  <% response=Processing.find(:all, :conditions=>["case_id=? and response_to=?",one.case_id,current_user.user_id])%>
	  <% if response != nil %>
	    <tr><td><%= link_to one.content, case_processing_path(:case_id=>one.case_id) %></td><td>Send by <%= one.created_by.strip()%> on <%= one.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td></tr>
	  <% end %>
	<% end %></table><br />
	
	<br />Other Cases
	<table cellspacing="10">
	<% @cases.each do |one|%>
	  <% response=Processing.find(:all, :conditions=>["case_id=? and response_to =?",one.case_id,current_user.user_id])%>
	  <% if response.nil? %>
	    <tr><td><%= link_to one.content, case_processing_path(:case_id=>one.case_id) %></td><td>Send by <%= one.created_by.strip()%> on <%= one.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td></tr>
	  <% end %>
	<% end %></table><br />
<% end %>

 </div>
</div>
